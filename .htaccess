RewriteOptions inherit
#<IfModule LiteSpeed>
#   CacheLookup on
#</IfModule>

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_HOST} jing.exelmed\.ph [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://jing.exelmed.ph/public/login/$1 [R,L]
</IfModule>
<IfModule mod_rewrite.c>
    RewriteEngine On

    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

